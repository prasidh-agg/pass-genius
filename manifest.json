{
  "manifest_version": 3,
  "name": "Password Generator",
  "version": "1.0",
  "description": "Generate strong passwords on the fly.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icons/icons8-lock-16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icons8-lock-16.png",
    "48": "icons/icons8-lock-48.png",
    "96": "icons/icons8-lock-96.png"
  }
}
